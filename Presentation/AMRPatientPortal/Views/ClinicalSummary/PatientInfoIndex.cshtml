        $(function() {
            $( "#tabs" ).tabs();
        });

        
        $(document).ready(function () 
        {
            
           $('#tabs').tabs();
            document.getElementById('tabs').style.display = 'block';
            var model = '@Html.Raw(Json.Encode(Model.FacilityVisitSelect))';
            // alert(model.facilitySelected);
            //alert(model.facilityOptions);
            // alert(model.visitSelected);
            // alert(model.visitOptions);
            if(model.facilitySelected != null && model.visitSelected != null)
            {
                //alert('inside!');
                var VisitHome = document.getElementById('cmbVisitsHome');
                VisitHome.innerHTML = model.visitOptions;
                var FacilityHome = document.getElementById('cmbFacilityHome');
           //     FacilityHome.innerHTML = model.facilityOptions;
                $('#cmbFacilityHome').val(model.facilitySelected);
                $('#cmbVisitsHome').val(model.visitSelected);

                var VisitHome = document.getElementById('cmbVisitsGlobal');
               // VisitHome.innerHTML = model.visitOptions;
                var FacilityHome = document.getElementById('cmbFacilityGlobal');
                FacilityHome.innerHTML = model.facilityOptions;
                $('#cmbFacilityGlobal').val(model.facilitySelected);
                $('#cmbVisitsGlobal').val(model.visitSelected);
            }
            if ($('#cmbFacilityHome option:selected').text() == 'Patient Entered') {

                // $('#cmbVisitsHome').attr('disabled', true);
                document.getElementById('cmbVisitsHome').disabled = true;
                $('#createaddsocial,#createaddfamily,#createaddpast,#createaddPOC,#createaddProcedures,#createaddClinicalInstructions,#createaddallergies,#createaddimmunizations,#createaddvitals,#createaddproblems,#createaddmedications').css('display', 'block');
              
            }
            else {
                $('#createaddsocial,#createaddfamily,#createaddPOC,#createaddpast,#createaddPOC,#createaddProcedures,#createaddClinicalInstructions,#createaddallergies,#createaddimmunizations,#createaddvitals,#createaddproblems,#createaddmedications').css('display', 'none');
          
                //$('#cmbVisitsHome').attr('disabled', false);
                document.getElementById('cmbVisitsHome').disabled = false;
            }
            var $tabs = $('#tabs').tabs();
            var selected = $tabs.tabs('option', 'active');
            $("#tabs").tabs({
                activate: function (event, ui) {
                    var selected = $tabs.tabs('option', 'active');
                    //ui.newTab.context.id;
                    switch (selected) {
                        case 0:
                            //alert('alert From Summary');
                         

                            var FacilityGlobal = document.getElementById('cmbFacilityGlobal');
                            var facilityoptions = FacilityGlobal.innerHTML;
                            var FacilityHome = document.getElementById('cmbFacilityHome');
                            FacilityHome.innerHTML = facilityoptions;
                            $('#cmbFacilityHome').val($('#cmbFacilityGlobal').val());

                            var VisitGlobal = document.getElementById('cmbVisitsGlobal');
                            var visitoptions = VisitGlobal.innerHTML;
                            var VisitHome = document.getElementById('cmbVisitsHome');
                            VisitHome.innerHTML = visitoptions;
                            $('#cmbVisitsHome').val($('#cmbVisitsGlobal').val());

                            if ($('#cmbFacilityHome option:selected').text() == 'Patient Entered') {
                                $('#cmbVisitsHome').attr('disabled', true);
                            }
                            else {
                                $('#cmbVisitsHome').attr('disabled', false);
                            }
                            var facilitySelected = $('#cmbFacilityHome option:selected').val();
                            var visitSelected = $('#cmbVisitsHome option:selected').val();
                            try {
                                ShowLoader();
                                //var IsPrimaryData =

                                var facilityId = $('#cmbFacilityHome option:selected').val();
                                var visitId = $('#cmbVisitsHome option:selected').val();
                                if ($('#cmbVisitsGlobal option:selected').val() == null) {
                                    visitId = 0;
                                }
                             
                                var requestData = {

                                    FacilityId: facilityId,
                                    VisitId: visitId

                                };


                                $.ajax({
                                    type: 'POST',
                                    url: 'home-widgets-filter/?visitId=' + requestData.VisitId + '&facilityId=' + requestData.FacilityId,
                                    dataType: 'json',
                                    contentType: 'application/json; charset=utf-8',
                                    success: function (data) {
                                        // if success

                                        $.each(data, function (index, item) {
                                            //alert( "Item Index = " + index + "Id = " + item.Id + "Item WidgetHtml" + item.WidgetHtml);
                                            $('#' + item.Id).empty();
                                            $('#' + item.Id).html(item.WidgetHtml);
                                        });

                                        $("#tbl-Appointments").fixedHeaderTable();
                                        $("#tbl-SocialHistory").fixedHeaderTable();
                                        $("#tbl-LabTests").fixedHeaderTable();
                                        $("#tbl-Statement").fixedHeaderTable();
                                        $("#tbl-Visits").fixedHeaderTable();
                                        $("#tbl-FamilyHistory").fixedHeaderTable();
                                        $("#tbl-Medications").fixedHeaderTable();
                                        $("#tbl-Allergies").fixedHeaderTable();
                                        $("#tbl-Documents").fixedHeaderTable();
                                        $("#tbl-Immunizations").fixedHeaderTable();
                                        $("#tbl-Insurance").fixedHeaderTable();
                                        $("#tbl-PastMedications").fixedHeaderTable();
                                        $("#tbl-poc").fixedHeaderTable();
                                        $("#tbl-Problems").fixedHeaderTable();
                                        $("#tbl-VitalSigns").fixedHeaderTable();
                                        $("#tbl-Procedure").fixedHeaderTable();
                                        HideLoader();

                                    },
                                    error: function (xhr, ajaxOptions, thrownError) {
                                        //if error
                                        var myCookie = getCookie(".Patient");

                                        if (myCookie == null) {
                                            // do cookie doesn't exist stuff;
                                            alert('Not logged in. Please log in to continue');
                                            window.location = '/Login'
                                            //xhr.getResponseHeader('Location');
                                        }
                                        else {

                                            alert('Error : ' + xhr.message);
                                        }
                                        HideLoader();
                                    },
                                    complete: function (data) {
                                        // if completed


                                    },
                                    async: false,
                                    processData: false
                                });
                            } catch (err) {

                                if (err && err !== "") {
                                    alert(err.message);
                                }
                            }
                            break;
                      
                      

                  
                     
                    }
                }
            });
            // var test = $('#social-portlet-tab').html();
            //$('#family-portlet-tab').html(test);
            //$("#family-portlet-tab #tab-SocialHistory").fixedHeaderTable();
            // 
            // 
            // 
            //

            //$("#tab-FamilyHistory").fixedHeaderTable({ width: '840', height: '320' });
            // 
            //
        });
       
    </script>

    


