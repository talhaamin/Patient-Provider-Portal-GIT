                                var requestData = {
                                    Password: $.trim($('#txtOldPassword').val()),
                                    NewPassword: $.trim($('#txtNewPassword').val())

                                };


                                $.ajax({
                                    type: 'POST',
                                    url: 'account-password-change',
                                    data: JSON.stringify(requestData),
