                                });
                            } catch (err) {

                                if (err && err !== "") {
                                    alert(err.message);
                                    HideLoader();
                                }
                            }
                            //   $(this).dialog("close");
                            ShowLoader();
                        }

                        //                        end ajax
                    },
                  
                },
                close: function () {
                   // $("#Passwordmsg").css("display", "none");
                    $("#txtOldPassword").removeClass("ui-state-error");
                }
            });

        });

