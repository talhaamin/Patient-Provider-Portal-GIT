             function Registration()
             {

                 var bValid = true;
                 if ($("#chkterms").prop('checked') == false) {
                     alert('Please Agree on terms & conditions');
                     return;
                 }
                 if (!Reqvalue($.trim($('#txtFname').val()), 'First Name')) {
                     return  ;
                 }

                 if (!Reqvalue($.trim($('#txtLname').val()), 'Last Name')) {
                     return;
                 }
             
                 if (!Reqvalue($.trim($('#adddob').val()), 'Date of Birthday')) {
                     return ;
                 }
                 if (!Reqvalue($.trim($('#txtAdd').val()), 'Address')) {
                     return ;
                 }
                 //if (!Reqvalue($.trim($('#txtAdd2').val()), 'Address Line 2')) {
                 //    return;
                 //}
                 if (!Reqvalue($.trim($('#txtCity').val()), 'City')) {
                     return ;
                 }
                 if (!Reqvalue($.trim($('#txtZipcode').val()), 'Zip Code')) {
                     return ;
                 }
                 if (!Reqvalue($.trim($('#txthomphon').val()), 'Phone Number')) {
                     return ;
                 }
                 if (!checkEmail($.trim($('#txtemail').val()))) {
                     return ;
                 }
                 if ($.trim($('#txtemail').val()) != $.trim($('#txtconemail').val())) {
                     alert('Please Enter a Valid Email Address')
                     return ;
                 }
           
                 $("#loader11").css("display", "block");
                 $("#Next").css("display", "none");

                 if (bValid) {
                     document.getElementById('loader11').style.display = 'block';

                     try {
                         var requestData = {
                             Promocode: $.trim($('#txtpromocode').val()),
                             Title: $.trim($('#cmbName').val()),
                             FirstName: $.trim($('#txtFname').val()),
                             MiddleName:  $.trim($('#txtMname').val()),
                             LastName: $.trim($('#txtLname').val()),
                             Suffix: $.trim($('#cmbSuffix').val()),                             
                             Address1:  $.trim($('#txtAdd').val()),
                             Address2:  $.trim($('#txtAdd2').val()),
                             County: $.trim($('#cmbCountry option:selected').text()),
                             CountryCode: $.trim($('#cmbCountry').val()),
                             State: $.trim($('#cmbStates option:selected').text()),
                             City:  $.trim($('#txtCity').val()),
                             Zip: $.trim($('#txtZipcode').val()),
                             HomePhone:  $.trim($('#txthomphon').val()),
                             EMail: $.trim($('#txtemail').val()),
                             MailAddress1: $.trim($('#txtAddopt1').val()),
                             MailAddress2: $.trim($('#txtAddopt2').val()),
                             MailState: $.trim($('#cmbStatesopt option:selected').text()),
                             MailCity:  $.trim($('#txtCityopt').val()),
                             MailPostalCode: $.trim($('#txtZipcodeopt').val()),
                             DOB: $.trim($('#adddob').val()),
                             MailCountryCode: $.trim($('#cmbCountryopt1 option:selected').val()),
                             Active: true,
                         };
                         var date = new Date();
                         var dat = date.getDate();
                         var month = date.getMonth()+1;
                         var year = date.getFullYear();
                         if (month < 10) { month = '0' + month;}
                         var currentdate = month + '-' + dat + '-' + year;
                         $.ajax({
                             type: 'POST',
                             url: 'patient-signup',
                             data: JSON.stringify(requestData),
                             dataType: 'json',
                             contentType: 'application/json; charset=utf-8',
                             success: function (data) {
                                 var RecurringAmount = parseFloat(data.RecurringAmount).toFixed(2);
                                 // if success
                                 $('input[name=Amount]').val(data.billrenewal1);
                                 $('input[name=FullName]').val(data.html);
                                 $('input[name=Address1]').val(data.add);
                                 $('input[name=Address2]').val(data.add2);
                                 $('input[name=City]').val(data.city);
                                 $('input[name=State]').val(data.State);
                                 $('input[name=Zip]').val(data.Zip);
                                 $('input[name=PhoneNumber]').val(data.phone);
                                 $('input[name=Email]').val(data.Email);
                                 $('input[name=WalletDescription]').val(data.WalletDescription);
                                 $('input[name=RecurringAmount]').val(RecurringAmount);
                                 $('input[name=StartDate]').val(currentdate);
                                 $('input[name=CustRefID]').val(data.patientid);
                           
                             
                             
                                 if (data.status == true) {
                                     JumptolastPage();
                                 }
                                 else {
                                     Return();
                                 }
                            

                                 // $("#current-med-grid").html(data);

                             },
                             error: function (xhr, ajaxOptions, thrownError) {
                                 //if error
                            
                                 alert('Error : ' + xhr.message);
                             },
                             complete: function (data) {
                                 // if completed
                            
                           
                             },
                             async: true,
                             processData: false
                         });
                  
                    
                     }
                 

                     catch (err) {

                         if (err && err !== "") {
                             alert(err.message);
                             HideLoader();
                         }
                     }
                     $(this).dialog("close");
                     Hide();
                 }

             }

         
