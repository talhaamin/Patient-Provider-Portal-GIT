      function checkKeycode(e) {


          if (!e) //Firefox and Safari gets argument directly.
          {
              e = window.event; // this is for IE
          }

          var keycode = e.keyCode ? e.keyCode : e.which;

          if (keycode == 116) {
              // for Ctrl +F5
              isClose = true;
              keyboardevent = true;
          }
          else if (keycode == 17) {
              // for Ctrl + W
              isClose = false;
              keyboardevent = true;
          }
